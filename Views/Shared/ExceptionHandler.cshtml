
@{
    ViewBag.Title = "NotFound";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<div class="card">
    <div class="card-header">
        <h2 class="card-title"><b>Application Error: </b></h2>
    </div>
    <div class="card-body">
        <div class="alert alert-warning alert-with-icon" data-notify="container">
            <i class="material-icons" data-notify="icon">notifications</i>
            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                <i class="material-icons">close</i>
            </button>
            <span data-notify="message" style="font-size:medium">Sorry, An error occured while processing your request. Please try again after sometime. <br /><br /> If the error perisists then contact the System Admin.</span>
        </div>
    </div>
</div>